{
	"name": "Vue Workspace",
	"dockerComposeFile": "docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/home/devcontainer",
	"customizations": {
		"vscode": {
			"extensions": [
				"googlecloudtools.cloudcode"
